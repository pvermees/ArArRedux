loadWiscArData <- function(dname){ 
    filenames <- Sys.glob(paste0(dname,"/*.RUN"))
    nf <- length(filenames)
    thedate <- rep(NA,nf)
    labels <- rep(NA,nf)
    thetime <- NULL
    d <- NULL
    for (i in 1:nf){
        con = file(filenames[i], "r")
        r <- 0
        while (TRUE){
            r <- r + 1
            line = readLines(con, n=1)
            if (length(line) == 0) {
                break
            } else if (r == daterow){
                txt <- unlist(strsplit(line,'#'))[2]
                thedate[i] <- readthedate(txt)
            } else if (r == tagrow){
                labels[i] <- unlist(strsplit(line,'"'))[2]
            }
        }
        close(con)
    }
    ##:ess-bp-start::browser@nil:##
browser(expr=is.null(.ESSBP.[["@30@"]]));##:ess-bp-end:##
    list(thedate=thedate,labels=labels)
}
